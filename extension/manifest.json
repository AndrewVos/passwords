{
  "manifest_version": 2,
  "name": "passwords",
  "description": "",
  "version": "0.1",
  "permissions": [ "contextMenus", "tabs", "clipboardWrite", "unlimitedStorage", "http://*/*", "https://*/*" ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["jquery-2.0.3.min.js", "mousetrap.min.js", "mousetrap-global-bind.min.js", "background.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' http://localhost:8080; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}


